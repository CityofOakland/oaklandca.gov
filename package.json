{
  "name": "oaklandca.gov",
  "version": "1.0.0",
  "description": "Website for oaklandca.gov",
  "main": "gulpfile.js",
  "author": "Christopher Kennedy, onebrightlight <chris@onebrightlight.com>",
  "copyright": "onebrightlight",
  "license": "UNLICENSED",
  "private": true,
  "paths": {
    "src": {
      "base": "./src/",
      "css": "./src/css/",
      "fonts": "./src/fonts/",
      "img": "./src/img/",
      "json": "./src/json/",
      "js": "./src/js/",
      "scss": "./src/scss/"
    },
    "dist": {
      "base": "./web/",
      "css": "./web/assets/css/",
      "js": "./web/assets/js/",
      "fonts": "./web/assets/fonts/",
      "img": "./web/assets/img/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "fonts": "./build/fonts/",
      "js": "./build/js/",
      "html": "./build/html/",
      "img": "./build/img/"
    },
    "favicon": {
      "src": "./src/img/favicon.png",
      "dest": "./web/img/site/",
      "path": "/img/site/"
    },
    "tailwindcss": {
      "src": "./src/css/tailwind.css",
      "build": "./build/css/tailwind/",
      "conf": "./tailwind.js"
    },
    "scss": [],
    "craftConfig": "./config/",
    "templates": "./templates/"
  },
  "urls": {
    "live": "https://oaklandca.gov/",
    "local": "http://oakland.test/",
    "critical": "http://oakland.test/"
  },
  "vars": {
    "siteCssName": "site.combined.min.css",
    "scssName": "style.scss",
    "cssName": "style.css"
  },
  "globs": {
    "distCss": [
      "./build/css/tailwind/*.css",
      "./src/css/site.css"
    ],
    "img": [
      "./web/img/"
    ],
    "components": [],
    "fonts": [
      "./src/fonts/*.{eot,ttf,woff,woff2}"
    ],
    "purgecss": [
      "./templates/**/*.{html,twig}"
    ],
    "purgecssWhitelist": [],
    "critical": [
      {
        "url": "",
        "template": "index"
      },
      {
        "url": "offline",
        "template": "offline"
      },
      {
        "url": "error",
        "template": "error"
      },
      {
        "url": "503",
        "template": "503"
      },
      {
        "url": "404",
        "template": "404"
      }
    ],
    "criticalWhitelist": [],
    "download": [
      {
        "url": "https://www.google-analytics.com/analytics.js",
        "dest": "./web/js/"
      }
    ],
    "distJs": [
      "./build/js/*.js"
    ],
    "babelJs": [
      "./src/js/*.js"
    ],
    "inlineJs": [],
    "siteIcon": "./web/img/site/favicon.*"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "git-rev-sync": "^1.12.0",
    "glob-all": "^3.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^4.0.15",
    "laravel-mix-banner": "^0.1.4",
    "laravel-mix-criticalcss": "^1.0.0",
    "laravel-mix-purgecss": "^4.1.0",
    "laravel-mix-tailwind": "^0.1.0",
    "moment": "^2.24.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "resolve-url-loader": "^3.0.1",
    "stylelint-config-recommended": "^2.1.0",
    "tailwindcss": "^0.7.4",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "hover.css": "^2.3.2",
    "instantsearch.js": "2.10.4"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  }
}
